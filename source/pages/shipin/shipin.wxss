/* pages/shipin/shipin.wxss */
.gen {
  width: 130rpx;
  height: 50rpx;
  background: #31B0DE;
  color: #ffffff;
  text-align: center;
  line-height: 50rpx;
  font-size: 18rpx;
  border-radius: 50rpx;
}
.send{
  /* width: 100vw; */
  height: 50rpx;
  z-index: 10;
  position: fixed;
  bottom: 0;
  background: #ffffff;
}
.mask {
  width: 100vw;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 5;
}
.pop {
  width: 100vw;
  height: 106rpx;
  background: #F1EFF2;
  position: fixed;
  bottom: 0;
  left: 0;
}
.zx {
  width: 160rpx;
  height: 60rpx;
  background: #31B0EB;
  color: #ffffff;
  text-align: center;
  line-height: 60rpx;
  border-radius: 60rpx;
}